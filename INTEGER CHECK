list_1=[1,2,3,4]
list_2=[2,3,2,6,4]
if len(list_1)==len(list_2):
    print("length of two list are equal\n")
else:
    print("legnth of two list are not equal \n")
print("sum of element in list1 is:",sum(list_1))
print("sum of element in list2 is:",sum(list_2))
if sum(list_1)==sum(list_2):
    print("sum of element of two list are equal")
else:
    print("sum of element are not equal \n\n")
for i in  list_1:
    if i in list_2:
        print(i)  
